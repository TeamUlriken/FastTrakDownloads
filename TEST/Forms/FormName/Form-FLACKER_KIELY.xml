          <Answer AnswerId="58941" OrderNumber="2" OptionText="Moderat svekkelse"/>
          <Answer AnswerId="58942" OrderNumber="4" OptionText="Kraftig svekkelse"/>
        </FormItem>
      </Item>
      <Item ItemId="1120" ItemType="2" VarName="FK_A_04" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemHeader="Toalettbesøk" ItemText="I hvilken grad er pasienten i stand til å gjennomføre toalettritualet, dette inkluderer både bekken, urinflaske, å vaske selv etter inkontinentepisoder, skifte bleie selv, håndtere stomi og kateter. Ordne klærne. Forflytning til og fra toalett vurderes ikke her." ItemHelp="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="0">
          <Answer AnswerId="58945" OrderNumber="0" OptionText="Ingen svekkelse"/>
          <Answer AnswerId="58946" OrderNumber="1" OptionText="Lett svekkelse"/>
          <Answer AnswerId="58947" OrderNumber="2" OptionText="Moderat svekkelse"/>
          <Answer AnswerId="58948" OrderNumber="4" OptionText="Kraftig svekkelse"/>
        </FormItem>
      </Item>
      <Item ItemId="1121" ItemType="2" VarName="FK_A_05" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemHeader="Hygiene" ItemText="Hvordan takler pasienten å ordne håret, pusse tennene, barbere seg, sminke seg, vaske og tørke ansikt og hender?" ItemHelp="Her vurderes imidlertid ikke gjennomføring av karbad og dusj." ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="0">
          <Answer AnswerId="58951" OrderNumber="0" OptionText="Ingen svekkelse"/>
          <Answer AnswerId="58952" OrderNumber="1" OptionText="Lett svekkelse"/>
          <Answer AnswerId="58953" OrderNumber="2" OptionText="Moderat svekkelse"/>
          <Answer AnswerId="58954" OrderNumber="4" OptionText="Kraftig svekkelse"/>
