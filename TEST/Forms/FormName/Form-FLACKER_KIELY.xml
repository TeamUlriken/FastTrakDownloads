      <Item ItemId="1118" ItemType="2" VarName="FK_A_02" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemHeader="Forflytning" ItemText="Bevege seg mellom forskjellige objekter og posisjoner, dette inkluderer til og fra; seng, stol, rullestol, stående, sittende." ItemHelp="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="0">
