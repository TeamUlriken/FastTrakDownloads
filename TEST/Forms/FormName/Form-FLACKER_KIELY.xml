      <Item ItemId="1119" ItemType="2" VarName="FK_A_03" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemHeader="Fødeinntak" ItemText="I hvilken grad inntas adekvat mengde føde (kvantitet og kvalitet), inkludert spising, drikking, iv føde og væske, føde via tube." ItemHelp="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="0">
