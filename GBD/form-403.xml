<question item_id="4827" mid="MUST_BMI_SCORE" datatype="QN" page_number="9" question_text="MUST BMI poeng (poeng)" optional="1" visible="-1" min_value="0" max_value="2" units="poeng" expression="ISNEG(BMI-18.5) + ISNEG(BMI-20)" group_id="MUST_BMI_SCORE" />
