<question item_id="4812" mid="DEMISPAN" datatype="QN" page_number="5" page_title="Estimering av h&#248;yde" header="M&#229;l fra halsgrop til tuppen av langfinger p&#229; ene side." question_text="Halvfavn (cm)" optional="0" min_value="60" max_value="110" units="cm" group_id="DEMISPAN" />
<question item_id="4814" mid="EHEIGHT_KNEE" datatype="QN" page_number="5" page_title="Estimering av h&#248;yde" question_text="Estimert h&#248;yde basert p&#229; kneh&#248;yde (cm)" optional="1" min_value="30" max_value="80" units="cm" decimals="1" expression="((64.19-(0.04*AGENOW)+(2.02*KNEHOYDE))*ISNULL(SEX-1) + (84.88-(0.24*AGENOW)+(1.83*KNEHOYDE))*ISNULL(SEX-2))*ISPOS(KNEHOYDE)" group_id="EHEIGHT_KNEE" />
<question item_id="4813" mid="EHEIGHT_DEMISPAN" datatype="QN" page_number="5" page_title="Estimering av h&#248;yde" question_text="Estimert h&#248;yde beregnet fra halvfavn (cm)" optional="1" units="cm" decimals="1" expression="((1.35 * DEMISPAN + 60.1)*ISNULL(SEX-2) + (1.40 * DEMISPAN + 57.8)*ISNULL(SEX-1)) * ISPOS(DEMISPAN)" group_id="EHEIGHT_DEMISPAN" />
<question item_id="4818" mid="CHEIGHT" datatype="QN" page_number="5" page_title="Estimering av h&#248;yde" question_text="H&#248;yde som inng&#229;r i beregning" optional="1" visible="-1" decimals="1" expression="HEIGHT * ISPOS(HEIGHT) + EHEIGHT_KNEE * ISNEG(HEIGHT) * ISPOS( EHEIGHT_KNEE - EHEIGHT_DEMISPAN )+ EHEIGHT_DEMISPAN * ISNEG(HEIGHT) * ISNEG(EHEIGHT_KNEE - EHEIGHT_DEMISPAN )" group_id="CHEIGHT" />
<question item_id="3310" mid="BMI" datatype="QN" page_number="6" page_title="MUST Beregning" question_text="BMI (kg/m2)" optional="1" min_value="5" max_value="100" units="kg/m2" decimals="1" expression="WEIGHT/((CHEIGHT/100)*(CHEIGHT/100)) * ISPOS(WEIGHT)*ISPOS(CHEIGHT)" help_text="Beregnes fra m&#229;lt h&#248;yde eller maks estimert h&#248;yde." group_id="BMI" />
<question item_id="4820" mid="WEIGHTPERCENTCHANGE" datatype="QN" page_number="7" question_text="Vektendring (%)" optional="1" units="%" expression="100*(WEIGHT-MAXWEIGHT_180D)/MAXWEIGHT_180D*ISPOS(MAXWEIGHT_180D)" group_id="WEIGHTPERCENTCHANGE" />
<question item_id="4829" mid="MUST_LOST5" datatype="ORD" page_number="7" question_text="Foreligger vekttap over 5%" optional="1" visible="-1" expression="2-ISNEG(5+WEIGHTPERCENTCHANGE)" enumeration_uid="4829" group_id="MUST_LOST5" />
