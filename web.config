<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.web>
		<customErrors mode="Off"/>
	</system.web>
	<system.webServer>
    <rewrite>
      <rules>
        <rule name="Binaries">
          <match url=".*exe"/>
          <action type="Redirect" url="http://binaries.fasttrak.no/{1}"/>
        </rule>
      </rules>
    </rewrite>
		<staticContent>
			<mimeMap fileExtension="fr3" mimeType="application/xml" />
			<mimeMap fileExtension="note" mimeType="application/xml" />
			<mimeMap fileExtension="xhtml" mimeType="application/xml" />
			<mimeMap fileExtension="sql" mimeType="text/plain" />
		</staticContent>
	</system.webServer>
</configuration>