<enumeration enumeration_uid="4558">
  <enum_item value="0" text="Kan nesten aldri angi riktig &#229;rstid." answer_id="24217" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24218" />
  <enum_item value="2" text="Kan angi riktig &#229;rstid." answer_id="24219" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24220" />
  <enum_item value="4" text="Kan angi &#229;r og m&#229;ned men som oftest ikke ukedag/dato." answer_id="24225" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24226" />
  <enum_item value="6" text="Fungerer uten problemer n&#229;r det gjelder tidsorientering, kjenner ukedag, m&#229;ned og &#229;r." answer_id="24227" />
</enumeration>

<enumeration enumeration_uid="4559">
  <enum_item value="0" text="Ikke orientert om egen alder." answer_id="24228" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24229" />
  <enum_item value="2" text="Opplever som oftest seg selv som gammel, men tar feil av alderen (+/-15 &#229;r)." answer_id="24230" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24231" />
  <enum_item value="4" text="Tar av og til feil av egen alder, men kan som oftest oppgi tiln&#230;rmet riktig alder (+/- 2 &#229;r)." answer_id="24855" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24856" />
  <enum_item value="6" text="Kan alltid oppgi sin egen alder korrekt." answer_id="24857" />
</enumeration>

<enumeration enumeration_uid="4562">
  <enum_item value="0" text="Kan ikke oppgi riktig sted." answer_id="24232" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24233" />
  <enum_item value="2" text="Kan av og til angi riktig sted (&quot;sykehjem&quot; o.l.)." answer_id="24234" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24235" />
  <enum_item value="4" text="Kan angi &quot;sykehjem&quot;,  &quot;sykehus&quot;, &quot;institusjon&quot; som oppholdssted." answer_id="24236" />
  <enum_item value="5" text="(mellom 4 og 6)." answer_id="24237" />
  <enum_item value="6" text="Kan angi riktig navn p&#229; institusjon." answer_id="24238" />
</enumeration>

<enumeration enumeration_uid="4563">
  <enum_item value="0" text="Kjenner ikke igjen n&#230;rmeste p&#229;r&#248;rende." answer_id="24264" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24265" />
  <enum_item value="2" text="Kjenner n&#230;rmeste p&#229;r&#248;rende." answer_id="24266" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24267" />
  <enum_item value="4" text="Kan alltid oppgi navn p&#229; n&#230;rmeste p&#229;r&#248;rende eller n&#230;re venner.  Kjenner igjen s&#248;sken, barn og barnebarn." answer_id="24268" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24269" />
  <enum_item value="6" text="Er godt orientert om sin egen familes og n&#230;r venners n&#229;v&#230;rende situasjon (Kjenner navn, sivilstand, bosted til f.eks. s&#248;sken, barn, barnebarn og oldebarn)." answer_id="24270" />
</enumeration>

<enumeration enumeration_uid="4566">
  <enum_item value="0" text="Kan ikke oppgi detaljer om egen person." answer_id="24271" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24272" />
  <enum_item value="2" text="Kan bare oppgi enkelte fragmenter om egen bakgrunn." answer_id="24273" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24274" />
  <enum_item value="4" text="Kan oppgi bakgrunnsopplysninger om yrke, sivilstand, tidligere arbeidplasser og f&#248;dselsdato." answer_id="24275" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24276" />
  <enum_item value="6" text="Er klar over sin bakgrunn som yrke, sivilstand, tidligere arbeidsplasser og bosteder." answer_id="24277" />
</enumeration>

<enumeration enumeration_uid="4568">
  <enum_item value="0" text="Finner ikke fram i avdelingen. (unntatt ved pr&#248;ving og feiling)" answer_id="24285" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24286" />
  <enum_item value="2" text="Kan av og til finne fram til WC, eget rom, spiserom." answer_id="24287" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24288" />
  <enum_item value="4" text="Finner alltid frem til WC, eget rom og spiserom." answer_id="24289" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24290" />
  <enum_item value="6" text="Finner alltid fram til alle rom i avdelingen og er orientert om hva som ligger i skuffer og skap p&#229; eget rom." answer_id="24291" />
</enumeration>

<enumeration enumeration_uid="4570">
  <enum_item value="0" text="Husker ikke fra dag til dag." answer_id="24382" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24383" />
  <enum_item value="2" text="Kan noen ganger huske enkelte fragmenter fra viktige/spesielle hendelser fra dag til dag." answer_id="24384" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24385" />
  <enum_item value="4" text="Husker som oftest detaljert fra viktige/spesielle hendelser fra dag til dag (f.eks. f&#248;dselsdager, bes&#248;k fra p&#229;r&#248;rende)." answer_id="24386" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24387" />
  <enum_item value="6" text="Husker alltid vesentlige hendelser fra dag til dag detaljert og sammenhengende." answer_id="24388" />
</enumeration>

<enumeration enumeration_uid="4571">
  <enum_item value="0" text="Husker omtrent ikke fra tidligere i livet." answer_id="24389" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24390" />
  <enum_item value="2" text="Husker noen enkeltdetaljer fra hendelser tidligere i livet." answer_id="24391" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24392" />
  <enum_item value="4" text="Husker som oftest noen detaljer fra hendelser tidligere i livet." answer_id="24393" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24394" />
  <enum_item value="6" text="Husker detaljert, presist og sammenhengende fra viktige/spesielle hendelser tidligere i livet." answer_id="24395" />
</enumeration>

<enumeration enumeration_uid="4572">
  <enum_item value="0" text="Oppfatter kun enkeltord." answer_id="24396" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24397" />
  <enum_item value="2" text="Oppfatter innholdet i enkle setninger." answer_id="24398" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24399" />
  <enum_item value="4" text="Forst&#229;r meningsinnholdet i en normal, dagligdags samtale." answer_id="24400" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24401" />
  <enum_item value="6" text="Forst&#229;r og kan delta aktivt og normalt i samtale om generelle tema som f.eks. samfunnsforhold." answer_id="24402" />
</enumeration>

<enumeration enumeration_uid="4573">
  <enum_item value="0" text="Snakker ikke." answer_id="24403" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24404" />
  <enum_item value="2" text="Som oftest vanskelig for &#229; snakke i hele setninger, uttrykker seg som oftest med enkeltord." answer_id="24405" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24406" />
  <enum_item value="4" text="Kan noen ganger bruke ufullstendige setninger eller ha problemer med &#229; finne enkelte ord." answer_id="24407" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24408" />
  <enum_item value="6" text="Bruker sammensatte setninger med normal flyt i talen. Utrykker seg presist. Ikke ordletingsproblemer." answer_id="24409" />
</enumeration>

<enumeration enumeration_uid="4574">
  <enum_item value="0" text="Kan ikke skrive." answer_id="24445" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24446" />
  <enum_item value="2" text="Skriver enkeltord (inkl. eget navn)." answer_id="24447" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24448" />
  <enum_item value="4" text="Skriver enkeltsetninger (ikke diktat)." answer_id="24449" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24450" />
  <enum_item value="6" text="Skriver sammenhengende tekst (f.eks. brev)." answer_id="24451" />
</enumeration>

<enumeration enumeration_uid="4575">
  <enum_item value="0" text="Sover st&#248;rstedelen av dagen." answer_id="24459" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24460" />
  <enum_item value="2" text="D&#248;sig store deler av dagen, kan sovne under samtale." answer_id="24461" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24462" />
  <enum_item value="4" text="D&#248;sig noen ganger om dagen." answer_id="24463" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24464" />
  <enum_item value="6" text="V&#229;ken om dagen." answer_id="24465" />
</enumeration>

<enumeration enumeration_uid="4576">
  <enum_item value="0" text="Leser ikke klokka." answer_id="24473" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24474" />
  <enum_item value="2" text="Vansker med &#229; lese klokka (f.eks. leser bare timer)." answer_id="24475" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24476" />
  <enum_item value="4" text="Leser riktig klokkeslett (timer, minutter, men knytter ikke klokkeslettet til tid p&#229; dagen)." answer_id="24477" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24478" />
  <enum_item value="6" text="Forst&#229;r og leser klokka korrekt, slik at det aldri er problem med at tidspunktet knyttes til tid p&#229; dagen." answer_id="24479" />
</enumeration>

<enumeration enumeration_uid="4577">
  <enum_item value="0" text="M&#229; ha hjelp til alt vask og stell." answer_id="24487" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24488" />
  <enum_item value="2" text="Vasker seg ufullstendig. Trenger konkret hjelp, selv n&#229;r situasjonen tilrettelegges." answer_id="24489" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24490" />
  <enum_item value="4" text="Vasker seg selv n&#229;r situasjonen tilrettelegges og det gis muntlig rettledning eller st&#248;tte." answer_id="24491" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24492" />
  <enum_item value="6" text="Ingen vansker med personlig hygiene. Kan vaske og stelle seg uten muntlig eller fysisk hjelp." answer_id="24493" />
</enumeration>

<enumeration enumeration_uid="4578">
  <enum_item value="0" text="M&#229; ha hjelp til all p&#229;kledning." answer_id="24501" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24502" />
  <enum_item value="2" text="Kler ufullstendig p&#229;/av seg. Trenger konkret hjelp, selv n&#229;r situasjonen tilrettelegges." answer_id="24503" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24504" />
  <enum_item value="4" text="Kler p&#229;/av seg n&#229;r situasjonen tilrettelegges og det gis muntlig rettledning eller st&#248;tte." answer_id="24505" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24506" />
  <enum_item value="6" text="Ingen vansker med av- og p&#229;kledning. Kan kle av/p&#229; seg uten muntlig eller fysisk hjelp." answer_id="24507" />
</enumeration>

<enumeration enumeration_uid="4579">
  <enum_item value="0" text="M&#229; helt eller delvis mates ." answer_id="24515" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24516" />
  <enum_item value="2" text="Spiser selv n&#229;r situasjonen tilrettelegges og/eller det gis muntlig hjelp/st&#248;tte." answer_id="24517" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24518" />
  <enum_item value="4" text="Noe problemer med spising, f.eks. vansker med bruk av bestikk, men spiser uten muntlig eller fysisk hjelp/tilrettelegging." answer_id="24519" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24520" />
  <enum_item value="6" text="Ingen problemer med spising." answer_id="24521" />
</enumeration>

<enumeration enumeration_uid="4580">
  <enum_item value="0" text="Inkontinent for urin og avf&#248;ring." answer_id="24529" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24530" />
  <enum_item value="2" text="Inkontinent for urin om natten, som oftest kontroll p&#229; vannlating om dagen." answer_id="24531" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24532" />
  <enum_item value="4" text="Som oftest kontroll p&#229; vannlating b&#229;de dag og natt (inkontinent mindre enn 3 g pr. uke, dag/natt)." answer_id="24533" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24534" />
  <enum_item value="6" text="Kontinent dag/natt." answer_id="24535" />
</enumeration>

<enumeration enumeration_uid="4581">
  <enum_item value="0" text="Alvorlig senket stemningsleie, nesten alltid fortvilet." answer_id="24543" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24544" />
  <enum_item value="2" text="Moderat senket stemningsleie, virker som oftest trist og nedfor." answer_id="24545" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24546" />
  <enum_item value="4" text="Lett senket stemningsleie, i uker trist og nedfor." answer_id="24547" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24548" />
  <enum_item value="6" text="Normalt stemningsleie." answer_id="24549" />
</enumeration>

<enumeration enumeration_uid="4582">
  <enum_item value="0" text="Kan g&#229; fysisk til angrep p&#229; personalet og medpasienter." answer_id="24550" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24551" />
  <enum_item value="2" text="Ofte sint og oppfarende. Kan v&#230;re truende/krevende overfor personer rundt seg, eller &#248;delegge ting." answer_id="24552" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24553" />
  <enum_item value="4" text="Kan av og til bli irritert og sint, kan skjelle ut personer rundt seg tilsynelatende uten grunn." answer_id="24554" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24555" />
  <enum_item value="6" text="Adekvate reaksjoner." answer_id="24556" />
</enumeration>

<enumeration enumeration_uid="4583">
  <enum_item value="0" text="Er nesten alltid redd og engstelig og/eller ofte panikkreaksjoner." answer_id="24557" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24558" />
  <enum_item value="2" text="Ofte engstelig i hverdagssituasjoner." answer_id="24559" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24560" />
  <enum_item value="4" text="Kan bli engstelig i nye situasjoner, ved unders&#248;kelser, i m&#248;te med nye mennesker e.l." answer_id="24561" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24562" />
  <enum_item value="6" text="Virker normalt trygg i dagliglivet." answer_id="24563" />
</enumeration>

<enumeration enumeration_uid="4584">
  <enum_item value="0" text="Kontinuerlig rastl&#248;s/urolig. G&#229;r hele tiden rundt dersom det er fysisk mulig, eller er hele tiden urolig i stol eller seng." answer_id="24564" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24565" />
  <enum_item value="2" text="Som oftest rastl&#248;s og urolig n&#229;r v&#229;ken." answer_id="24566" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24567" />
  <enum_item value="4" text="Av og til rastl&#248;s og urolig." answer_id="24568" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24569" />
  <enum_item value="6" text="Ikke rastl&#248;s og urolig." answer_id="24570" />
</enumeration>

<enumeration enumeration_uid="4585">
  <enum_item value="0" text="Ikke aktiv i daglige aktiviteter selv med kontinuerlig motivering." answer_id="24571" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24572" />
  <enum_item value="2" text="Trenger kontinuerlig motivering for &#229; utf&#248;re daglige aktiviteter." answer_id="24573" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24574" />
  <enum_item value="4" text="M&#229; som oftest hjelpes til &#229; komme i gang med daglige aktiviteter, men lar seg lett motivere til &#229; gj&#248;re noe." answer_id="24575" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24576" />
  <enum_item value="6" text="Tar selv initiativ og ansvar for &#229; utf&#248;re daglige aktiviteter som f.eks. telefonere, skrive brev, handle, vaske opp, lese, se p&#229; TV, h&#248;re radio, h&#229;ndarbeid." answer_id="24577" />
</enumeration>

<enumeration enumeration_uid="4586">
  <enum_item value="0" text="Deltar ikke i sosial aktivitet." answer_id="24613" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24614" />
  <enum_item value="2" text="Passiv deltakelse, f&#248;lger med i det som foreg&#229;r." answer_id="24615" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24616" />
  <enum_item value="4" text="Deltar i sosialt samv&#230;r satt i gang av andre." answer_id="24617" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24618" />
  <enum_item value="6" text="Tar initiativ til samv&#230;r med andre." answer_id="24619" />
</enumeration>

<enumeration enumeration_uid="4587">
  <enum_item value="0" text="Ikke sammen med andre." answer_id="24627" />
  <enum_item value="1" text="(mellom 0 og 2)" answer_id="24628" />
  <enum_item value="2" text="Sammen med naboer/medbeboere kun n&#229;r det tilrettelegges av andre." answer_id="24629" />
  <enum_item value="3" text="(mellom 2 og 4)" answer_id="24630" />
  <enum_item value="4" text="Er av og til sammen med naboer/medbeboere, ingen spesielle kontakter." answer_id="24631" />
  <enum_item value="5" text="(mellom 4 og 6)" answer_id="24632" />
  <enum_item value="6" text="Har et godt forhold til en eller flere naboer/medbeboere.  Er ofte sammen - gj&#248;r ting sammen." answer_id="24633" />
</enumeration>

