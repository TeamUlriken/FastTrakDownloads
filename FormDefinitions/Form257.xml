  <Form FormId="257" FormName="LABDATA" FormTitle="Laboratoriedata" Copyright="Norsk diabetesregister for voksne">
    <Page PageId="327" PageNumber="1" PageTitle="Antropometri*">
      <Item ItemId="3224" ItemType="1" VarName="WEIGHT" LoincScale="QN" UnitStr="kg" DisplayFormat="">
        <FormItem FormItemId="1396" OrderNumber="1" ItemText="Vekt (kg)" ItemHelp="Uten sko og yttertøy." MinExpression="10" MaxExpression="400" ExcludeFromText="0" ExcludeCaption="0" Visibility="0" CarryForward="1" ReadOnly="1" Highlight="1" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="328" PageNumber="2" PageTitle="Metabolsk status">
      <Item ItemId="527" ItemType="1" VarName="HBA1C" LoincScale="QN" UnitStr="%" DisplayFormat="">
        <FormItem FormItemId="291" OrderNumber="2" ItemText="B-HbA1c (%)" ItemHelp="HbA1c-verdien gjenspeiler det gjennomsnittlige blodsukkeret de siste 6 - 8 uker før prøvetakingen. Høye verdier ses derfor ved økt gjennomsnittlig blodglukose. Lave verdier kan ses ved økt hemoglobinomsetning som ved hemolytiske anemier." MinExpression="3" MaxExpression="25" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="1294" ItemType="1" VarName="CHOL" LoincScale="QN" UnitStr="mmol/L" DisplayFormat="">
        <FormItem FormItemId="546" OrderNumber="3" ItemText="S-Kolesterol (mmol/L)" ItemHelp="Høye verdier ved primære og sekundære hyperlipoproteinemier. De sekundære ses bl.a. ved hypothyreose, nefrotisk syndrom, obstruktive leversykdommer og diabetes mellitus. Lave verdier ved hyperthyreose, malabsorpsjon, medfødt beta-lipoproteinmangel (acanthocytose)." MinExpression="0.5" MaxExpression="30" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="1296" ItemType="1" VarName="HDL" LoincScale="QN" UnitStr="mmol/L" DisplayFormat="">
        <FormItem FormItemId="555" OrderNumber="4" ItemText="S-HDL-kolesterol (mmol/L)" ItemHelp="Høye verdier betyr oftest minsket risiko for hjerte-/karsykdom. De kan være arvelig betinget og kan også skyldes østrogenpåvirkning eller høy fysisk aktivitet. Alkoholinntak tenderer også til å øke HDL-kolesterol. Lave verdier kan skyldes inflammatoriske prosesser, særlig virushepatitt. De kan også være arvelig betinget (Tangiers sykdom, LCAT-mangel)." MinExpression="0" MaxExpression="15" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="2">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="1884" ItemType="1" VarName="LDL" LoincScale="QN" UnitStr="mmol/l" DisplayFormat="">
        <FormItem FormItemId="950" OrderNumber="5" ItemText="S-LDL-kolesterol (mmol/l)" ItemHelp="Høye verdier ved primære og sekundære hyperlipoproteinemier. De sekundære ses blant annet ved hypothyreose, nefrotisk syndrom, obstruktive leversykdommer og diabetes mellitus. Lave verdier ved hyperthyreose, malabsorpsjon, medfødt beta-lipoproteinmangel (acanthocytose)." MinExpression="0.2" MaxExpression="20" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="3488" ItemType="1" VarName="TRIG" LoincScale="QN" UnitStr="mmol/l" DisplayFormat="">
        <FormItem FormItemId="1946" OrderNumber="6" ItemText="S-Triglycerider (mmol/l)" ItemHelp="fastende eller ikke-fastende" MinExpression="0.1" MaxExpression="100" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="2">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4203" ItemType="1" VarName="CHOLRATIO" LoincScale="QN" DisplayFormat="">
        <FormItem FormItemId="2794" OrderNumber="7" ItemText="Totalkolesterol/HDL-kolesterol" ItemHelp="" Expression="CHOL/HDL" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="329" PageNumber="3" PageTitle="Leverprøver">
      <Item ItemId="1438" ItemType="1" VarName="ASAT" LoincScale="QN" UnitStr="U/L" DisplayFormat="">
        <FormItem FormItemId="682" OrderNumber="8" ItemText="S-ASAT (U/L)" ItemHelp="Høye verdier ved hjerteinfarkt med maksimum etter 2 døgn. ASAT er også økt ved hepatitt, toksisk leverskade, leverstuvning, myopati og lungeemboli. Det individuelle ASAT-nivået er relatert til muskelmasse. Maksimal fysisk aktivitet gir en stigning på 10-20 U/L. Østrogenholdige P-piller gir en stigning på opptil 10 U/L." MinExpression="1" MaxExpression="10000" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="1439" ItemType="1" VarName="ALAT" LoincScale="QN" UnitStr="U/L" DisplayFormat="">
        <FormItem FormItemId="687" OrderNumber="9" ItemText="S-ALAT (U/L)" ItemHelp="Høye verdier ved akutt og residiverende hepatitt, toksisk leverskade, mononukleose, galleveisobstruksjon." MinExpression="1" MaxExpression="10000" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="330" PageNumber="4" PageTitle="Nyrefunksjon">
      <Item ItemId="3304" ItemType="1" VarName="CREAT" LoincScale="QN" UnitStr="µmol/L" DisplayFormat="">
        <FormItem FormItemId="1557" OrderNumber="10" ItemText="S-Kreatinin (µmol/L)" ItemHelp="Høye verdier ved nedsatt glomerulusfiltrasjon: prerenalt ved sirkulasjonssvikt, renalt ved glomerulusskade, postrenalt ved avløpshinder. Lave verdier har liten klinisk interesse. Forekommer ved atrofisk muskulatur og i svangerskapet." MinExpression="10" MaxExpression="2000" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="3311" ItemType="1" VarName="UACR" LoincScale="QN" UnitStr="mg/mmol" DisplayFormat="">
        <FormItem FormItemId="1579" OrderNumber="11" ItemText="U-Albumin/Kreatinin-ratio (mg/mmol)" ItemHelp="" MinExpression="0" MaxExpression="7500" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Highlight="1" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="3313" ItemType="1" VarName="EGFR" LoincScale="QN" UnitStr="mL/min/1,73m2" DisplayFormat="">
        <FormItem FormItemId="1582" OrderNumber="12" ItemText="Estimert GFR (mL/min/1,73m2)" ItemHelp="Cockgroft-Gaults formel" Expression="(140-AGE)*WEIGHT/CREAT*(1.04*ISNULL(SEX-2)+1.23*ISNULL(SEX-1))" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
