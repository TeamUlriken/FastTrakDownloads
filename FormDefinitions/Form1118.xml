    <Page PageTitle="NEWS2 - Skåringsskjema for å fange opp potensielle risikopasienter" PageIntroduction="En lav skår utelukker ikke alvorlig sykdom. Skåringsskjema er et supplerende hjelpemiddel for å bedømme vitale funksjoner hos voksne pasienter.">
      <Item ItemId="1530" ItemType="2" VarName="NEWS01" LoincScale="ORD">
        <FormItem ItemText="Respirasjonsfrekvens:" ItemHelp="" Expression="ISPOS (SP_RESP_FREKVENS - 24) +  ISPOS (SP_RESP_FREKVENS - 20) + ISPOS (SP_RESP_FREKVENS - 11) + ISPOS (SP_RESP_FREKVENS - 8) + ISPOS (SP_RESP_FREKVENS - 2)" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1">
          <Answer AnswerId="10465" OrderNumber="1" OptionText="&lt;= 8 /min" ShortCode="3p" Score="3.00"/>
          <Answer AnswerId="10650" OrderNumber="2" OptionText="9 - 11 /min" ShortCode="1p" Score="1.00"/>
          <Answer AnswerId="10651" OrderNumber="3" OptionText="12 - 20 /min" ShortCode="0p" Score="0.00"/>
          <Answer AnswerId="10652" OrderNumber="4" OptionText="21 - 24 /min" ShortCode="2p" Score="2.00"/>
          <Answer AnswerId="10653" OrderNumber="5" OptionText="25 + /min" ShortCode="3p" Score="3.00"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageTitle="New*">
      <Item ItemId="1621" ItemType="2" VarName="NEWS02" LoincScale="ORD">
        <FormItem ItemText="SaO2 - Skala 1:" ItemHelp="Normal skala" Expression="(ISNULL (NEWS2_HYPERCAPNIA-2))*(ISPOS (O2_SATURATION - 95) + ISPOS (O2_SATURATION - 93) + ISPOS (O2_SATURATION - 91) + ISPOS (O2_SATURATION - 59))" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1">
          <Answer AnswerId="10654" OrderNumber="1" OptionText="&lt;= 91 %" ShortCode="3p" Score="3.00"/>
          <Answer AnswerId="10655" OrderNumber="2" OptionText="92 - 93 %" ShortCode="2p" Score="2.00"/>
          <Answer AnswerId="10656" OrderNumber="3" OptionText="94 - 95  %" ShortCode="1p" Score="1.00"/>
          <Answer AnswerId="10657" OrderNumber="4" OptionText="96 % +" ShortCode="0p" Score="0.00"/>
        </FormItem>
      </Item>
      <Item ItemId="2842" ItemType="2" VarName="NEWS02b" LoincScale="ORD">
        <FormItem ItemText="SaO2 - Skala 2:" ItemHelp="Optimal verdi her er satt til 88-92% på grunn av KOLS eller annen lungesykdom" Expression="(ISNULL (NEWS2_HYPERCAPNIA-1))&#xA;*&#xA;(&#xA;ISPOS (O2_SATURATION - 59) + &#xA;ISPOS (O2_SATURATION - 83) + &#xA;ISPOS (O2_SATURATION - 85) + &#xA;ISPOS (O2_SATURATION - 87) +&#xA;&#xA;(ISNULL (O2_SUPPLEMENT-3)) * (ISPOS (O2_SATURATION - 92)) &#xA;+ &#xA;ISPOS(O2_SATURATION - 92) * ((ISNULL (O2_SUPPLEMENT-2)) * (1 + ISPOS (O2_SATURATION - 92) + ISPOS (O2_SATURATION - 94) + ISPOS (O2_SATURATION - 96))) &#xA;+&#xA;ISPOS(O2_SATURATION - 92) * ((ISNULL (O2_SUPPLEMENT-1)) * (1 + ISPOS (O2_SATURATION - 92) + ISPOS (O2_SATURATION - 94) + ISPOS (O2_SATURATION - 96)))&#xA;)&#xA;" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1">
          <Answer AnswerId="60877" OrderNumber="1" OptionText="&lt;= 83 %" ShortCode="3p" Score="3.00"/>
          <Answer AnswerId="60878" OrderNumber="2" OptionText="84 - 85 %" ShortCode="2p" Score="2.00"/>
          <Answer AnswerId="60879" OrderNumber="3" OptionText="86 - 87  %" ShortCode="1p" Score="1.00"/>
          <Answer AnswerId="60880" OrderNumber="4" OptionText="88 - 92 %" ShortCode="0p" Score="0.00"/>
          <Answer AnswerId="11856" OrderNumber="5" OptionText="&gt;=93 % med romluft" ShortCode="0p" Score="0.00"/>
          <Answer AnswerId="11857" OrderNumber="6" OptionText="93 - 94 % med O2" ShortCode="1p" Score="1.00"/>
          <Answer AnswerId="11858" OrderNumber="7" OptionText="95 - 96 % med O2" ShortCode="2p" Score="2.00"/>
          <Answer AnswerId="11859" OrderNumber="8" OptionText="&gt;= 97 % med O2" ShortCode="3p" Score="3.00"/>
        </FormItem>
      </Item>
      <Item ItemId="1405" ItemType="2" VarName="NEWS03" LoincScale="ORD">
        <FormItem ItemText="Oksygentilførsel" ItemHelp="" Expression="ISNULL (O2_SUPPLEMENT - 1) + ISNULL (O2_SUPPLEMENT - 2) + (ISNULL (O2_SUPPLEMENT - 3))*2" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1">
