        <FormItem ItemText="Percentage body fat" ItemHelp="" Expression="BFM / ABW * 100.0" MinExpression="1" MaxExpression="99" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1" Decimals="2">
