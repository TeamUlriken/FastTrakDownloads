    <Page PageTitle="F. Følelsesmessig fungering">
      <Item ItemId="4581" ItemType="2" VarName="OBSDEM_STEMNING" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Stemningsleie" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24543" OrderNumber="0" OptionText="Alvorlig senket stemningsleie, nesten alltid fortvilet."/>
          <Answer AnswerId="24544" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24545" OrderNumber="2" OptionText="Moderat senket stemningsleie, virker som oftest trist og nedfor."/>
          <Answer AnswerId="24546" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24547" OrderNumber="4" OptionText="Lett senket stemningsleie, i uker trist og nedfor."/>
          <Answer AnswerId="24548" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24549" OrderNumber="6" OptionText="Normalt stemningsleie."/>
        </FormItem>
      </Item>
      <Item ItemId="4599" ItemType="4" VarName="OBSDEM_NOTE18" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til stemningsleie" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4582" ItemType="2" VarName="OBSDEM_IRRITASJON" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Irritasjon/utagering" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24550" OrderNumber="0" OptionText="Kan gå fysisk til angrep på personalet og medpasienter."/>
          <Answer AnswerId="24551" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24552" OrderNumber="2" OptionText="Ofte sint og oppfarende. Kan være truende/krevende overfor personer rundt seg, eller ødelegge ting."/>
          <Answer AnswerId="24553" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24554" OrderNumber="4" OptionText="Kan av og til bli irritert og sint, kan skjelle ut personer rundt seg tilsynelatende uten grunn."/>
          <Answer AnswerId="24555" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24556" OrderNumber="6" OptionText="Adekvate reaksjoner."/>
        </FormItem>
      </Item>
      <Item ItemId="4601" ItemType="4" VarName="OBSDEM_NOTE19" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til irritasjon/utagering" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4583" ItemType="2" VarName="OBSDEM_ANGST" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Angst" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24557" OrderNumber="0" OptionText="Er nesten alltid redd og engstelig og/eller ofte panikkreaksjoner."/>
          <Answer AnswerId="24558" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24559" OrderNumber="2" OptionText="Ofte engstelig i hverdagssituasjoner."/>
          <Answer AnswerId="24560" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24561" OrderNumber="4" OptionText="Kan bli engstelig i nye situasjoner, ved undersøkelser, i møte med nye mennesker e.l."/>
          <Answer AnswerId="24562" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24563" OrderNumber="6" OptionText="Virker normalt trygg i dagliglivet."/>
        </FormItem>
      </Item>
      <Item ItemId="4600" ItemType="4" VarName="OBSDEM_NOTE20" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til angst" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4584" ItemType="2" VarName="OBSDEM_RASTLOS" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Rastløshet/uro" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24564" OrderNumber="0" OptionText="Kontinuerlig rastløs/urolig. Går hele tiden rundt dersom det er fysisk mulig, eller er hele tiden urolig i stol eller seng."/>
          <Answer AnswerId="24565" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24566" OrderNumber="2" OptionText="Som oftest rastløs og urolig når våken."/>
          <Answer AnswerId="24567" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24568" OrderNumber="4" OptionText="Av og til rastløs og urolig."/>
          <Answer AnswerId="24569" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24570" OrderNumber="6" OptionText="Ikke rastløs og urolig."/>
        </FormItem>
      </Item>
