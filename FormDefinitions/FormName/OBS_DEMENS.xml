      <Item ItemId="4585" ItemType="2" VarName="OBSDEM_INITIATIV" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Initiativ" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24571" OrderNumber="0" OptionText="Ikke aktiv i daglige aktiviteter selv med kontinuerlig motivering."/>
          <Answer AnswerId="24572" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24573" OrderNumber="2" OptionText="Trenger kontinuerlig motivering for å utføre daglige aktiviteter."/>
          <Answer AnswerId="24574" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24575" OrderNumber="4" OptionText="Må som oftest hjelpes til å komme i gang med daglige aktiviteter, men lar seg lett motivere til å gjøre noe."/>
          <Answer AnswerId="24576" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24577" OrderNumber="6" OptionText="Tar selv initiativ og ansvar for å utføre daglige aktiviteter som f.eks. telefonere, skrive brev, handle, vaske opp, lese, se på TV, høre radio, håndarbeid."/>
        </FormItem>
      </Item>
