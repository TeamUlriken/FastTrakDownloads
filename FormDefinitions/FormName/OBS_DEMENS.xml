      <Item ItemId="4587" ItemType="2" VarName="OBSDEM_NABOER" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Forhold til naboer/medbeboere" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24627" OrderNumber="0" OptionText="Ikke sammen med andre."/>
          <Answer AnswerId="24628" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24629" OrderNumber="2" OptionText="Sammen med naboer/medbeboere kun når det tilrettelegges av andre."/>
          <Answer AnswerId="24630" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24631" OrderNumber="4" OptionText="Er av og til sammen med naboer/medbeboere, ingen spesielle kontakter."/>
          <Answer AnswerId="24632" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24633" OrderNumber="6" OptionText="Har et godt forhold til en eller flere naboer/medbeboere.  Er ofte sammen - gjør ting sammen."/>
        </FormItem>
      </Item>
