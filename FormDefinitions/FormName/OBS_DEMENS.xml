    <Page PageTitle="G. Sosial fungering">
      <Item ItemId="4586" ItemType="2" VarName="OBSDEM_SOSIALT" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Deltagelse i sosial aktivitet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24613" OrderNumber="0" OptionText="Deltar ikke i sosial aktivitet."/>
          <Answer AnswerId="24614" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24615" OrderNumber="2" OptionText="Passiv deltakelse, følger med i det som foregår."/>
          <Answer AnswerId="24616" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24617" OrderNumber="4" OptionText="Deltar i sosialt samvær satt i gang av andre."/>
          <Answer AnswerId="24618" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24619" OrderNumber="6" OptionText="Tar initiativ til samvær med andre."/>
        </FormItem>
