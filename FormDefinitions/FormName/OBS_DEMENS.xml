<?xml version="1.0"?>
<StudyForm>
  <Form FormId="380" FormName="OBS_DEMENS" FormTitle="OBS Demens">
    <Page PageTitle="A. Orientering">
      <Item ItemId="4558" ItemType="2" VarName="OBSDEM_TID" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Tidsorientering - måned/årstid" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="1" Optional="0">
          <Answer AnswerId="24217" OrderNumber="0" OptionText="Kan nesten aldri angi riktig årstid."/>
          <Answer AnswerId="24218" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24219" OrderNumber="2" OptionText="Kan angi riktig årstid."/>
          <Answer AnswerId="24220" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24225" OrderNumber="4" OptionText="Kan angi år og måned men som oftest ikke ukedag/dato."/>
          <Answer AnswerId="24226" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24227" OrderNumber="6" OptionText="Fungerer uten problemer når det gjelder tidsorientering, kjenner ukedag, måned og år."/>
        </FormItem>
      </Item>
      <Item ItemId="4560" ItemType="4" VarName="OBSDEM_NOTE1" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til tidsorientering" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4559" ItemType="2" VarName="OBSDEM_ALDER" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Alder" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="1" Optional="0">
          <Answer AnswerId="24228" OrderNumber="0" OptionText="Ikke orientert om egen alder."/>
          <Answer AnswerId="24229" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24230" OrderNumber="2" OptionText="Opplever som oftest seg selv som gammel, men tar feil av alderen (+/-15 år)."/>
          <Answer AnswerId="24231" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24855" OrderNumber="4" OptionText="Tar av og til feil av egen alder, men kan som oftest oppgi tilnærmet riktig alder (+/- 2 år)."/>
          <Answer AnswerId="24856" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24857" OrderNumber="6" OptionText="Kan alltid oppgi sin egen alder korrekt."/>
        </FormItem>
      </Item>
      <Item ItemId="4561" ItemType="4" VarName="OBSDEM_NOTE2" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til alder" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4562" ItemType="2" VarName="OBSDEM_STED" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Sted" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24232" OrderNumber="0" OptionText="Kan ikke oppgi riktig sted."/>
          <Answer AnswerId="24233" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24234" OrderNumber="2" OptionText="Kan av og til angi riktig sted (&quot;sykehjem&quot; o.l.)."/>
          <Answer AnswerId="24235" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24236" OrderNumber="4" OptionText="Kan angi &quot;sykehjem&quot;,  &quot;sykehus&quot;, &quot;institusjon&quot; som oppholdssted."/>
          <Answer AnswerId="24237" OrderNumber="5" OptionText="(mellom 4 og 6)."/>
          <Answer AnswerId="24238" OrderNumber="6" OptionText="Kan angi riktig navn på institusjon."/>
        </FormItem>
      </Item>
      <Item ItemId="4564" ItemType="4" VarName="OBSDEM_NOTE3" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til sted" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4563" ItemType="2" VarName="OBSDEM_FAMILIE" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Egen familie" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24264" OrderNumber="0" OptionText="Kjenner ikke igjen nærmeste pårørende."/>
          <Answer AnswerId="24265" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24266" OrderNumber="2" OptionText="Kjenner nærmeste pårørende."/>
          <Answer AnswerId="24267" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24268" OrderNumber="4" OptionText="Kan alltid oppgi navn på nærmeste pårørende eller nære venner.  Kjenner igjen søsken, barn og barnebarn."/>
          <Answer AnswerId="24269" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24270" OrderNumber="6" OptionText="Er godt orientert om sin egen familes og nær venners nåværende situasjon (Kjenner navn, sivilstand, bosted til f.eks. søsken, barn, barnebarn og oldebarn)."/>
        </FormItem>
      </Item>
      <Item ItemId="4565" ItemType="4" VarName="OBSDEM_NOTE4" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til egen familie" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4566" ItemType="2" VarName="OBSDEM_IDENTITET" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Egen identitet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24271" OrderNumber="0" OptionText="Kan ikke oppgi detaljer om egen person."/>
          <Answer AnswerId="24272" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24273" OrderNumber="2" OptionText="Kan bare oppgi enkelte fragmenter om egen bakgrunn."/>
          <Answer AnswerId="24274" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24275" OrderNumber="4" OptionText="Kan oppgi bakgrunnsopplysninger om yrke, sivilstand, tidligere arbeidplasser og fødselsdato."/>
          <Answer AnswerId="24276" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24277" OrderNumber="6" OptionText="Er klar over sin bakgrunn som yrke, sivilstand, tidligere arbeidsplasser og bosteder."/>
        </FormItem>
      </Item>
      <Item ItemId="4567" ItemType="4" VarName="OBSDEM_NOTE5" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til egen identitet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4568" ItemType="2" VarName="OBSDEM_AVDELING" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Avdelingsmiljøet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24285" OrderNumber="0" OptionText="Finner ikke fram i avdelingen. (unntatt ved prøving og feiling)"/>
          <Answer AnswerId="24286" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24287" OrderNumber="2" OptionText="Kan av og til finne fram til WC, eget rom, spiserom."/>
          <Answer AnswerId="24288" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24289" OrderNumber="4" OptionText="Finner alltid frem til WC, eget rom og spiserom."/>
          <Answer AnswerId="24290" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24291" OrderNumber="6" OptionText="Finner alltid fram til alle rom i avdelingen og er orientert om hva som ligger i skuffer og skap på eget rom."/>
        </FormItem>
      </Item>
      <Item ItemId="4569" ItemType="4" VarName="OBSDEM_NOTE6" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til avdelingsmiljøet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageTitle="B. Hukommelse">
      <Item ItemId="4570" ItemType="2" VarName="OBSDEM_NÆRHUK" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Nærhukommelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24382" OrderNumber="0" OptionText="Husker ikke fra dag til dag."/>
          <Answer AnswerId="24383" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24384" OrderNumber="2" OptionText="Kan noen ganger huske enkelte fragmenter fra viktige/spesielle hendelser fra dag til dag."/>
          <Answer AnswerId="24385" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24386" OrderNumber="4" OptionText="Husker som oftest detaljert fra viktige/spesielle hendelser fra dag til dag (f.eks. fødselsdager, besøk fra pårørende)."/>
          <Answer AnswerId="24387" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24388" OrderNumber="6" OptionText="Husker alltid vesentlige hendelser fra dag til dag detaljert og sammenhengende."/>
        </FormItem>
      </Item>
      <Item ItemId="4588" ItemType="4" VarName="OBSDEM_NOTE7" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til nærhukommelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4571" ItemType="2" VarName="OBSDEM_FJERNHUK" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Fjernhukommelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24389" OrderNumber="0" OptionText="Husker omtrent ikke fra tidligere i livet."/>
          <Answer AnswerId="24390" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24391" OrderNumber="2" OptionText="Husker noen enkeltdetaljer fra hendelser tidligere i livet."/>
          <Answer AnswerId="24392" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24393" OrderNumber="4" OptionText="Husker som oftest noen detaljer fra hendelser tidligere i livet."/>
          <Answer AnswerId="24394" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24395" OrderNumber="6" OptionText="Husker detaljert, presist og sammenhengende fra viktige/spesielle hendelser tidligere i livet."/>
        </FormItem>
      </Item>
      <Item ItemId="4589" ItemType="4" VarName="OBSDEM_NOTE8" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til fjernhukommelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageTitle="C. Språk og kommunikasjon">
      <Item ItemId="4572" ItemType="2" VarName="OBSDEM_SPRÅK" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Språkforståelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24396" OrderNumber="0" OptionText="Oppfatter kun enkeltord."/>
          <Answer AnswerId="24397" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24398" OrderNumber="2" OptionText="Oppfatter innholdet i enkle setninger."/>
          <Answer AnswerId="24399" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24400" OrderNumber="4" OptionText="Forstår meningsinnholdet i en normal, dagligdags samtale."/>
          <Answer AnswerId="24401" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24402" OrderNumber="6" OptionText="Forstår og kan delta aktivt og normalt i samtale om generelle tema som f.eks. samfunnsforhold."/>
        </FormItem>
      </Item>
      <Item ItemId="4590" ItemType="4" VarName="OBSDEM_NOTE9" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til språkforståelse" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4573" ItemType="2" VarName="OBSDEM_TALE" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Taleflyt" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24403" OrderNumber="0" OptionText="Snakker ikke."/>
          <Answer AnswerId="24404" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24405" OrderNumber="2" OptionText="Som oftest vanskelig for å snakke i hele setninger, uttrykker seg som oftest med enkeltord."/>
          <Answer AnswerId="24406" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24407" OrderNumber="4" OptionText="Kan noen ganger bruke ufullstendige setninger eller ha problemer med å finne enkelte ord."/>
          <Answer AnswerId="24408" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24409" OrderNumber="6" OptionText="Bruker sammensatte setninger med normal flyt i talen. Utrykker seg presist. Ikke ordletingsproblemer."/>
        </FormItem>
      </Item>
      <Item ItemId="4591" ItemType="4" VarName="OBDEM_NOTE10" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til taleflyt" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4574" ItemType="2" VarName="OBSDEM_SKRIVE" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Skriving" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24445" OrderNumber="0" OptionText="Kan ikke skrive."/>
          <Answer AnswerId="24446" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24447" OrderNumber="2" OptionText="Skriver enkeltord (inkl. eget navn)."/>
          <Answer AnswerId="24448" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24449" OrderNumber="4" OptionText="Skriver enkeltsetninger (ikke diktat)."/>
          <Answer AnswerId="24450" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24451" OrderNumber="6" OptionText="Skriver sammenhengende tekst (f.eks. brev)."/>
        </FormItem>
      </Item>
      <Item ItemId="4592" ItemType="4" VarName="OBSDEM_NOTE11" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til skriving" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageTitle="D. Oppfatte og forstå">
      <Item ItemId="4575" ItemType="2" VarName="OBSDEM_VAKEN" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Våkenhet om dagen" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24459" OrderNumber="0" OptionText="Sover størstedelen av dagen."/>
          <Answer AnswerId="24460" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24461" OrderNumber="2" OptionText="Døsig store deler av dagen, kan sovne under samtale."/>
          <Answer AnswerId="24462" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24463" OrderNumber="4" OptionText="Døsig noen ganger om dagen."/>
          <Answer AnswerId="24464" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24465" OrderNumber="6" OptionText="Våken om dagen."/>
        </FormItem>
      </Item>
      <Item ItemId="4593" ItemType="4" VarName="OBSDEM_NOTE12" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til våkenhet" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4576" ItemType="2" VarName="OBSDEM_KLOKKA" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Forstå klokka" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24473" OrderNumber="0" OptionText="Leser ikke klokka."/>
          <Answer AnswerId="24474" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24475" OrderNumber="2" OptionText="Vansker med å lese klokka (f.eks. leser bare timer)."/>
          <Answer AnswerId="24476" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24477" OrderNumber="4" OptionText="Leser riktig klokkeslett (timer, minutter, men knytter ikke klokkeslettet til tid på dagen)."/>
          <Answer AnswerId="24478" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24479" OrderNumber="6" OptionText="Forstår og leser klokka korrekt, slik at det aldri er problem med at tidspunktet knyttes til tid på dagen."/>
        </FormItem>
      </Item>
      <Item ItemId="4594" ItemType="4" VarName="OBSDEM_NOTE13" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til klokka" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageTitle="E. Selvhjelp">
      <Item ItemId="4577" ItemType="2" VarName="OBSDEM_HYGIENE" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Personlig hygiene" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24487" OrderNumber="0" OptionText="Må ha hjelp til alt vask og stell."/>
          <Answer AnswerId="24488" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24489" OrderNumber="2" OptionText="Vasker seg ufullstendig. Trenger konkret hjelp, selv når situasjonen tilrettelegges."/>
          <Answer AnswerId="24490" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24491" OrderNumber="4" OptionText="Vasker seg selv når situasjonen tilrettelegges og det gis muntlig rettledning eller støtte."/>
          <Answer AnswerId="24492" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24493" OrderNumber="6" OptionText="Ingen vansker med personlig hygiene. Kan vaske og stelle seg uten muntlig eller fysisk hjelp."/>
        </FormItem>
      </Item>
      <Item ItemId="4595" ItemType="4" VarName="OBSDEM_NOTE14" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til personlig hygiene" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4578" ItemType="2" VarName="OBSDEM_KLÆR" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Av- og påkledning" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24501" OrderNumber="0" OptionText="Må ha hjelp til all påkledning."/>
          <Answer AnswerId="24502" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24503" OrderNumber="2" OptionText="Kler ufullstendig på/av seg. Trenger konkret hjelp, selv når situasjonen tilrettelegges."/>
          <Answer AnswerId="24504" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24505" OrderNumber="4" OptionText="Kler på/av seg når situasjonen tilrettelegges og det gis muntlig rettledning eller støtte."/>
          <Answer AnswerId="24506" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24507" OrderNumber="6" OptionText="Ingen vansker med av- og påkledning. Kan kle av/på seg uten muntlig eller fysisk hjelp."/>
        </FormItem>
      </Item>
      <Item ItemId="4596" ItemType="4" VarName="OBSDEM_NOTE15" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notat til av- og påkledning" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4579" ItemType="2" VarName="OBSDEM_MAT" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Måltider" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24515" OrderNumber="0" OptionText="Må helt eller delvis mates ."/>
          <Answer AnswerId="24516" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24517" OrderNumber="2" OptionText="Spiser selv når situasjonen tilrettelegges og/eller det gis muntlig hjelp/støtte."/>
          <Answer AnswerId="24518" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24519" OrderNumber="4" OptionText="Noe problemer med spising, f.eks. vansker med bruk av bestikk, men spiser uten muntlig eller fysisk hjelp/tilrettelegging."/>
          <Answer AnswerId="24520" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24521" OrderNumber="6" OptionText="Ingen problemer med spising."/>
        </FormItem>
      </Item>
      <Item ItemId="4597" ItemType="4" VarName="OBSDEM_NOTE16" LoincScale="NAR" DisplayFormat="" Multiline="1">
        <FormItem ItemText="Notalt til måltider" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4580" ItemType="2" VarName="OBSDEM_KONTINENS" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem ItemText="Kontinens" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="24529" OrderNumber="0" OptionText="Inkontinent for urin og avføring."/>
          <Answer AnswerId="24530" OrderNumber="1" OptionText="(mellom 0 og 2)"/>
          <Answer AnswerId="24531" OrderNumber="2" OptionText="Inkontinent for urin om natten, som oftest kontroll på vannlating om dagen."/>
          <Answer AnswerId="24532" OrderNumber="3" OptionText="(mellom 2 og 4)"/>
          <Answer AnswerId="24533" OrderNumber="4" OptionText="Som oftest kontroll på vannlating både dag og natt (inkontinent mindre enn 3 g pr. uke, dag/natt)."/>
          <Answer AnswerId="24534" OrderNumber="5" OptionText="(mellom 4 og 6)"/>
          <Answer AnswerId="24535" OrderNumber="6" OptionText="Kontinent dag/natt."/>
        </FormItem>
      </Item>
      <Item ItemId="4598" ItemType="4" VarName="OBSDEM_NOTE17" LoincScale="NAR" DisplayFormat="" Multiline="1">
