<?xml version="1.0" encoding="ISO-8859-1"?>
<StudyForm>
  <Form FormId="403" FormName="MUST" FormTitle="MUST - Underernæring">
    <Page PageId="1014" PageNumber="1" PageTitle="Høyde og vekt">
      <Item ItemId="3225" ItemType="1" VarName="HEIGHT" LoincScale="QN" UnitStr="cm" DisplayFormat="">
        <FormItem FormItemId="1429" OrderNumber="1" ItemText="Høyde" ItemHelp="Uten sko." MinExpression="50" MaxExpression="220" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="1" Optional="0">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4817" ItemType="2" VarName="HEIGHT_MEASURED" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3544" OrderNumber="2" ItemText="Er høyde registrert?" ItemHelp="" Expression="2-ISPOS(HEIGHT)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer AnswerId="26969" OrderNumber="1" OptionText="Ja"/>
          <Answer AnswerId="26970" OrderNumber="2" OptionText="Nei" ShortCode="N"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1015" PageNumber="2" PageTitle="Vekt*">
      <Item ItemId="3224" ItemType="1" VarName="WEIGHT" LoincScale="QN" UnitStr="kg" DisplayFormat="">
        <FormItem FormItemId="1411" OrderNumber="3" ItemText="Vekt" ItemHelp="Uten sko og yttertøy." MinExpression="10" MaxExpression="400" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4819" ItemType="1" VarName="MAXWEIGHT_180D" LoincScale="QN" UnitStr="kg" DisplayFormat="">
        <FormItem FormItemId="3546" OrderNumber="4" ItemText="Høyeste vekt siste 6 mnd (kg)" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4826" ItemType="2" VarName="WEIGHT_MEASURED" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3553" OrderNumber="5" ItemText="Er vekt registrert" ItemHelp="" Expression="2-ISPOS(WEIGHT)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer AnswerId="26975" OrderNumber="1" OptionText="Ja"/>
          <Answer AnswerId="26976" OrderNumber="2" OptionText="Nei" ShortCode="N"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1016" PageNumber="3" PageTitle="Vekt fra tidligere journal">
      <Item ItemId="4821" ItemType="1" VarName="LASTWEIGHT" LoincScale="QN" UnitStr="kg" DisplayFormat="">
        <FormItem FormItemId="3548" OrderNumber="6" ItemText="Siste vekt fra journal (kg)" ItemHelp="" Expression="DATA.WEIGHT" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4822" ItemType="1" VarName="WEIGHTAGE_DAYS" LoincScale="QN" UnitStr="dager" DisplayFormat="">
        <FormItem FormItemId="3549" OrderNumber="7" ItemText="Dager siden siste veiing (dager)" ItemHelp="" Expression="NOW-DATA.WEIGHT.TIMESTAMP" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4823" ItemType="1" VarName="CWEIGHT" LoincScale="QN" UnitStr="kg" DisplayFormat="">
        <FormItem FormItemId="3550" OrderNumber="8" ItemText="Vekt til bruk i beregninger (kg)" ItemHelp="" Expression="ISPOS(WEIGHT)*WEIGHT + ISNEG(WEIGHT)*DATA.WEIGHT*ISNEG(WEIGHTAGE_DAYS-30)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4825" ItemType="2" VarName="WEIGHT_TOO_OLD" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3552" OrderNumber="9" ItemText="Er vekten fra journal for gammel" ItemHelp="" Expression="2-ISPOS(WEIGHTAGE_DAYS-30)*(1-ISPOS(WEIGHT))" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer AnswerId="26971" OrderNumber="1" OptionText="Ja"/>
          <Answer AnswerId="26972" OrderNumber="2" OptionText="Nei" ShortCode="N"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1017" PageNumber="4" PageTitle="For gammel vekt*">
      <Item ItemId="4824" ItemType="8" VarName="MSG_WEIGHT_OLD" LoincScale="MSG" DisplayFormat="">
        <FormItem FormItemId="3551" OrderNumber="10" ItemText="Vekten fra journal er for gammel." ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1018" PageNumber="5" PageTitle="Estimering av høyde">
      <Item ItemId="4811" ItemType="1" VarName="KNEHOYDE" LoincScale="QN" UnitStr="cm" DisplayFormat="">
        <FormItem FormItemId="3540" OrderNumber="11" ItemText="Knehøyde (cm)" ItemHelp="Mål fra fotsålen til over kneet med 90 vinkel i både ankel og kne." MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4812" ItemType="1" VarName="DEMISPAN" LoincScale="QN" UnitStr="cm" DisplayFormat="">
        <FormItem FormItemId="3541" OrderNumber="12" ItemHeader="Mål fra halsgrop til tuppen av langfinger på ene side." ItemText="Halvfavn (cm)" ItemHelp="" MinExpression="60" MaxExpression="110" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4814" ItemType="1" VarName="EHEIGHT_KNEE" LoincScale="QN" UnitStr="cm" DisplayFormat="">
        <FormItem FormItemId="3543" OrderNumber="13" ItemText="Estimert høyde basert på knehøyde (cm)" ItemHelp="" Expression="((64.19-(0.04*AGENOW)+(2.02*KNEHOYDE))*ISNULL(SEX-1) +&#xA;(84.88-(0.24*AGENOW)+(1.83*KNEHOYDE))*ISNULL(SEX-2))*ISPOS(KNEHOYDE)" MinExpression="30" MaxExpression="80" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4813" ItemType="1" VarName="EHEIGHT_DEMISPAN" LoincScale="QN" UnitStr="cm" DisplayFormat="">
        <FormItem FormItemId="3542" OrderNumber="14" ItemText="Estimert høyde beregnet fra halvfavn (cm)" ItemHelp="" Expression="((1.35 * DEMISPAN + 60.1)*ISNULL(SEX-2) +&#xA;(1.40 * DEMISPAN + 57.8)*ISNULL(SEX-1)) * ISPOS(DEMISPAN)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4818" ItemType="1" VarName="CHEIGHT" LoincScale="QN" DisplayFormat="">
        <FormItem FormItemId="3545" OrderNumber="15" ItemText="Høyde som inngår i beregning" ItemHelp="" Expression="HEIGHT * ISPOS(HEIGHT) +&#xA;EHEIGHT_KNEE * ISNEG(HEIGHT) * ISPOS( EHEIGHT_KNEE - EHEIGHT_DEMISPAN )+&#xA;EHEIGHT_DEMISPAN * ISNEG(HEIGHT) * ISNEG(EHEIGHT_KNEE - EHEIGHT_DEMISPAN )" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1019" PageNumber="6" PageTitle="MUST Beregning">
      <Item ItemId="3310" ItemType="1" VarName="BMI" LoincScale="QN" UnitStr="kg/m2" DisplayFormat="">
        <FormItem FormItemId="1574" OrderNumber="16" ItemText="BMI" ItemHelp="Beregnes fra målt høyde eller maks estimert høyde." Expression="WEIGHT/((CHEIGHT/100)*(CHEIGHT/100)) * ISPOS(WEIGHT)*ISPOS(CHEIGHT)" MinExpression="5" MaxExpression="150" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1" Decimals="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1020" PageNumber="7" PageTitle="Vektendring*">
      <Item ItemId="4820" ItemType="1" VarName="WEIGHTPERCENTCHANGE" LoincScale="QN" UnitStr="%" DisplayFormat="">
        <FormItem FormItemId="3547" OrderNumber="17" ItemText="Vektendring (%)" ItemHelp="" Expression="100*(WEIGHT-MAXWEIGHT_180D)/MAXWEIGHT_180D*ISPOS(MAXWEIGHT_180D)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4829" ItemType="2" VarName="MUST_LOST5" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3556" OrderNumber="18" ItemText="Foreligger vekttap over 5%" ItemHelp="" Expression="2-ISNEG(5+WEIGHTPERCENTCHANGE)" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer AnswerId="26981" OrderNumber="1" OptionText="Ja"/>
          <Answer AnswerId="26982" OrderNumber="2" OptionText="Nei" ShortCode="N"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1021" PageNumber="8" PageTitle="Ufrivillig vekttap*">
      <Item ItemId="4830" ItemType="2" VarName="MUST_INVOLUNTARY" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3557" OrderNumber="19" ItemText="Er vekttapet ufrivillig?" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="26983" OrderNumber="1" OptionText="Ja" VerboseText="Ufrivillig vekttap." ShortCode="J"/>
          <Answer AnswerId="26984" OrderNumber="2" OptionText="Nei" VerboseText="Frivillig vekttap." ShortCode="N"/>
        </FormItem>
      </Item>
    </Page>
    <Page PageId="1022" PageNumber="9" PageTitle="Beregning forts*">
      <Item ItemId="4831" ItemType="2" VarName="MUST_CRISIS" LoincScale="ORD" DisplayFormat="Vertical">
        <FormItem FormItemId="3558" OrderNumber="20" ItemText="Akutt sykdom" ItemHelp="Hvis pasienten lider av akutt sykdom og ikke har hatt eller sannsynligvis ikke vil ha næringsinntak i fem dager eller mer." MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="26987" OrderNumber="1" OptionText="Ja" VerboseText="Akutt sykdom." ShortCode="J" Score="2.00"/>
          <Answer AnswerId="26988" OrderNumber="2" OptionText="Nei" ShortCode="N" Score="0.00"/>
        </FormItem>
      </Item>
      <Item ItemId="4827" ItemType="1" VarName="MUST_BMI_SCORE" LoincScale="QN" UnitStr="poeng" DisplayFormat="">
        <FormItem FormItemId="3554" OrderNumber="21" ItemText="MUST BMI poeng (poeng)" ItemHelp="" Expression="ISNEG(BMI-18.5) + ISNEG(BMI-20)" MinExpression="0" MaxExpression="2" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4828" ItemType="1" VarName="MUST_WEIGHT_SCORE" LoincScale="QN" UnitStr="poeng" DisplayFormat="">
        <FormItem FormItemId="3555" OrderNumber="22" ItemText="MUST Vekttap (poeng)" ItemHelp="Vekttap &gt; 10% gir 2 poeng, &gt; 5% gir 1 poeng." Expression="(ISNEG(10+WEIGHTPERCENTCHANGE) + ISNEG(5+WEIGHTPERCENTCHANGE)) * ISNULL(MUST_INVOLUNTARY-1)" MinExpression="0" MaxExpression="2" ExcludeFromText="0" ExcludeCaption="0" Visibility="-1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
      <Item ItemId="4832" ItemType="1" VarName="MUST_TOTAL_SCORE" LoincScale="QN" UnitStr="poeng" DisplayFormat="">
        <FormItem FormItemId="3559" OrderNumber="23" ItemText="MUST total score (poeng)" ItemHelp="0 = Lav risiko, 1 = Middels risiko (observasjon ), 2+ = Høy risiko (start behandling)" Expression="MUST_BMI_SCORE + MUST_WEIGHT_SCORE + MUST_CRISIS_SCORE" MinExpression="0" MaxExpression="6" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="1">
          <Answer/>
        </FormItem>
      </Item>
    </Page>
  </Form>
