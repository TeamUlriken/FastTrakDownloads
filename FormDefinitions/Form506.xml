    <Page PageTitle="BIMS - Brief Interview for Mental Status">
      <Item ItemId="6129" ItemType="2" VarName="MDS30_C2" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemHeader="Gjenta tre ord" ItemText="Si til beboer: &quot;Jeg skal si tre ord som du skal huske.  Kan du repetere ordene etter at jeg har sagt alle tre.  Ordene er SOKK, BLÅ og SENG.  Nå skal du si de tre ordene." ItemHelp="Scoring er resultatet på FØRSTE forsøk.  Etter første forsøk, gjenta ordene med hint (sokk, noe å ha på; blå, en farge; seng, et møbel;).  Du kan gjenta ordene opp til to ganger til." MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="32263" OrderNumber="0" OptionText="Ingen" VerboseText="Husker ingen ord."/>
          <Answer AnswerId="32264" OrderNumber="1" OptionText="Ett" VerboseText="Husker ett ord."/>
          <Answer AnswerId="32265" OrderNumber="2" OptionText="To" VerboseText="Husker to ord."/>
          <Answer AnswerId="32266" OrderNumber="3" OptionText="Tre" VerboseText="Husker tre ord."/>
        </FormItem>
      </Item>
      <Item ItemId="6130" ItemType="2" VarName="MDS30_C3a" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemText="Si til beboer: &quot;Kan du si hvilket år vi er i nå?&quot;" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="32271" OrderNumber="0" OptionText="Mer enn 5 år feil, eller ikke noe svar."/>
          <Answer AnswerId="32272" OrderNumber="1" OptionText="Oppgir 2 til 5 år feil."/>
          <Answer AnswerId="32273" OrderNumber="2" OptionText="Ett år feil."/>
          <Answer AnswerId="32274" OrderNumber="3" OptionText="Riktig år."/>
        </FormItem>
      </Item>
      <Item ItemId="6131" ItemType="2" VarName="MDS30_C3b" LoincScale="ORD" DisplayFormat="">
        <FormItem ItemText="Si til beboer: &quot;Kan du si hvilken måned vi er i nå?&quot;" ItemHelp="" MinExpression="" MaxExpression="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" Optional="0">
          <Answer AnswerId="32277" OrderNumber="0" OptionText="Mer enn 1 mnd feil, eller ikke noe svar."/>
          <Answer AnswerId="32278" OrderNumber="1" OptionText="Fra 6 dager til 1 mnd feil."/>
          <Answer AnswerId="32279" OrderNumber="2" OptionText="Riktig innenfor 5 dager."/>
          <Answer AnswerId="32280" OrderNumber="3" OptionText="Riktig måned."/>
        </FormItem>
      </Item>
      <Item ItemId="6132" ItemType="2" VarName="MDS30_C3c" LoincScale="ORD" DisplayFormat="">
