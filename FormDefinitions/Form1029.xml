      <Item ItemId="3031" ItemType="2" VarName="VAR3031" LoincScale="ORD">
        <FormItem ItemText="Beste tid på 4m gangtest" ItemHelp="" Expression="(ISNULL (TIME1_BEST-2) * (ISNEG (TIME1_WALK4M - 4.82) + ISNEG (TIME1_WALK4M - 6.20) + ISNEG (TIME1_WALK4M - 8.70) + ISPOS (TIME1_WALK4M) + 1)) +&#xA;(ISNULL (TIME2_BEST-2) * (ISNEG (TIME2_WALK4M - 4.82) + ISNEG (TIME2_WALK4M - 6.20) + ISNEG (TIME2_WALK4M - 8.70) + ISPOS (TIME2_WALK4M) + 1))" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="1">
