          <Answer AnswerId="12061" OrderNumber="2" OptionText="Tid &gt; 8.7 sekunder" Score="1.00"/>
          <Answer AnswerId="12062" OrderNumber="3" OptionText="Tid 6.21 - 8.70 sekunder" Score="2.00"/>
          <Answer AnswerId="12063" OrderNumber="4" OptionText="Tid 4.82 - 6.20 sekunder" Score="3.00"/>
          <Answer AnswerId="12064" OrderNumber="5" OptionText="Tid &lt; 4.82 sekunder" Score="4.00"/>
