      <Item ItemId="3657" ItemType="2" VarName="VAR3657" LoincScale="ORD">
        <FormItem ItemHeader="Skåring klokketest" ItemText="Beskriv klokketegningen" ItemHelp="" ExcludeFromText="0" ExcludeCaption="0" Visibility="1" CarryForward="0" ReadOnly="0" Optional="0">
          <Answer AnswerId="61418" OrderNumber="0" OptionText="Det settes ingen tall, kun bokstaver, markeringsstreker og/eller symboler eller ingenting på klokken." Score="0.00"/>
          <Answer AnswerId="61419" OrderNumber="1" OptionText="Visere feil og tall gjentatt, stokket om på og/eller utelatt tross gjentatt instruksjon, tall over 24, tall kun i halve sirkelen og/eller ingen visere." Score="1.00"/>
          <Answer AnswerId="61420" OrderNumber="2" OptionText="Tall riktig/nesten riktig, men kun én viser eller mer enn to visere, eller alle tall 1-24 etter hverandre i én sirkel også etter retest." Score="2.00"/>
          <Answer AnswerId="61421" OrderNumber="3" OptionText="Tall riktig/nesten riktig, men visere feil, eller visere riktig, men kun tall 12-23/13-24 eller 1-12 i snudd rekkefølge også etter retest." Score="3.00"/>
          <Answer AnswerId="61422" OrderNumber="4" OptionText="Små plasseringsfeil tall og/eller visere, blandede arabiske og romertall, ombyttede visere, tall både i og utenfor sirkel." Score="4.00"/>
          <Answer AnswerId="61423" OrderNumber="5" OptionText="Alle tall (1-12, I-XII eller doble tallsett 1-24: 1-12 og 13-24) på rett plass enten i eller utenfor sirkel og korrekt plasserte visere." Score="5.00"/>
        </FormItem>
      </Item>
